[Music]
label=
command=~/.config/i3blocks/playing.py --layout t-a-A-p
separator_block_width=13
interval=5
color=#e7e7eb

[volume]
label=
command=~/.config/i3blocks/volume
interval=5
signal=1

[battery]
command=~/.config/i3blocks/battery2
markup=pango
interval=10

[time]
label=
command=date +'%m/%d %H:%M'
color=#e7e7eb
interval=5
